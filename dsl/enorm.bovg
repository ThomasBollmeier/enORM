<!-- Bovinus grammar for enORM persistent object definition -->

<!-- Tokens -->

word VARNAME "[_a-zA-Z][_a-zA-Z0-9]*";
separator CURLY_LBRACE "{";
separator CURLY_RBRACE "}";

<!-- Production Rules -->

@grammar
enorm = persobjdef*;

persobjdef = 'persistentobject' objname=VARNAME CURLY_LBRACE
    CURLY_RBRACE;